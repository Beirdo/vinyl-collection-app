{
    "name": "vinyl-collection-app",
    "version": "1.0.0",
    "description": "Vinyl Collection App (Phonegap/React/Browserify/Gulp)",
    "repository": {
        "type": "git",
        "url": "git@github.com:Beirdo/vinyl-collection-app.git"
    },
    "keywords": [
        "vinyl",
        "phonegap",
        "react",
        "browserify"
    ],
    "author": "Gavin Hurlbut",
    "license": "MIT",
    "dependencies": {
        "cordova": "~3.4.1-0.1.0",
        "underscore": "~1.1.7",
        "jquery": "~2.1.4",
        "react": "~0.14.8",
        "react-dom": "~0.14.8",
        "aws4": "~1.3.2",
        "aws-sdk": "~2.3.0"
    },
    "devDependencies": {
        "aliasify": "~1.9.0",
        "babelify": "~7.2.0",
        "babel-preset-react": "~6.5.0",
        "browserify": "~13.0.0",
        "browserify-shim": "~3.8.12",
        "envify": "~3.4.0",
        "gulp": "~3.9.1",
        "gulp-changed": "~1.3.0",
        "gulp-imagemin": "~2.4.0",
        "gulp-less": "~3.0.5",
        "gulp-notify": "~2.2.0",
        "gulp-recess": "~1.2.0",
        "gulp-util": "~3.0.7",
        "notify-send": "~0.1.2",
        "querystring-browser": "~1.0.3",
        "reactify": "~1.1.1",
        "vinyl-source-stream": "~1.1.0"
    },
    "aliasify": {
        "aliases": {
            "querystring": "querystring-browser"
        },
        "verbose": true,
        "global": true,
        "appliesTo": {"includeExtensions" : [".js"]}
    },
    "browserify": {
        "transform": [
            [
                "reactify",
                {
                    "es6": true
                }
            ],
            "browserify-shim"
        ]
    },
    "browserify-shim": {
        "plugin": {
            "exports": "plugin",
            "depends": [
                "jquery:$"
            ]
        }
    }
}
